{
  "expo": {
    "name": "Expo Mega Demo",
    "slug": "expo-mega-demo",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "demo",
    "platforms": ["android", "ios"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "ios": {
      "bundleIdentifier": "com.barthap.expomegademo",
      "supportsTablet": false
    },
    "android": {
      "jsEngine": "jsc",
      "package": "com.barthap.expomegademo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "@config-plugins/react-native-ble-plx",
      "expo-dev-client",
      "./patches/fix-entitlements.plugin",
      "./custom_native_modules/expo-music-picker/app.plugin"
    ]
  },
  "name": "expo-mega-demo"
}
