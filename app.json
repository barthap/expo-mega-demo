{
  "expo": {
    "name": "Expo Mega Demo",
    "slug": "expo-mega-demo",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "demo",
    "platforms": [
      "android",
      "ios"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "bundleIdentifier": "com.barthap.expomegademo",
      "supportsTablet": false
    },
    "android": {
      "package": "com.barthap.expomegademo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "react-native-ble-plx",
      "./patches/fix-entitlements.plugin",
      "./custom_native_modules/expo-music-picker/app.plugin",
      "expo-audio"
    ]
  }
}
